{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Hangfire": {
    "WorkerCount": 3,
    "AutomaticRetryAttempts": 3,
    "SucceededJobsRetentionDays": 30,
    "DeletedJobsRetentionDays": 30,
    "FailedJobsRetentionDays": 90
  },

  "AllowedHosts": "*"
}
