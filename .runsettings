<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="Code Coverage" uri="datacollector://Microsoft/CodeCoverage/2.0" assemblyQualifiedName="Microsoft.VisualStudio.Coverage.DynamicCoverageDataCollector, Microsoft.VisualStudio.TraceCollector, Version=11.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a">
        <Configuration>
          <CodeCoverage>
            <!-- Exclude obj, bin, and tests folders -->
            <ModulePaths>
              <Exclude>
                <ModulePath>.*\\obj\\.*</ModulePath>
                <ModulePath>.*\\bin\\.*</ModulePath>
                <ModulePath>.*\\tests\\.*</ModulePath>
              </Exclude>
            </ModulePaths>
            
            <!-- Additional exclusions for common build artifacts -->
            <Functions>
              <Exclude>
                <!-- Exclude auto-generated code -->
                <Function>.*\.g\.cs$</Function>
                <Function>.*\.Designer\.cs$</Function>
              </Exclude>
            </Functions>
            
            <Attributes>
              <Exclude>
                <!-- Exclude generated code -->
                <Attribute>^System\.Diagnostics\.DebuggerHidden.*</Attribute>
                <Attribute>^System\.Diagnostics\.DebuggerNonUserCode.*</Attribute>
                <Attribute>^System\.CodeDom\.Compiler\.GeneratedCode.*</Attribute>
                <Attribute>^System\.Diagnostics\.CodeAnalysis\.ExcludeFromCodeCoverage.*</Attribute>
              </Exclude>
            </Attributes>
            
            <!-- Use SymbolSearch to improve performance -->
            <UseVerifiableInstrumentation>True</UseVerifiableInstrumentation>
            <AllowLowIntegrityProcesses>True</AllowLowIntegrityProcesses>
            <CollectFromChildProcesses>True</CollectFromChildProcesses>
            <CollectAspDotNet>False</CollectAspDotNet>
          </CodeCoverage>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>
